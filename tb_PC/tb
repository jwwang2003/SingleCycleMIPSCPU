S_0x125f06e50 .scope module, "PC_tb" "PC_tb" 2 3;
 .timescale -9 -12;
v0x125f174a0_0 .var "PCin", 31 0;
v0x125f17550_0 .net "PCout", 31 0, v0x125f17240_0;  1 drivers
v0x125f17600_0 .var "clock", 0 0;
v0x125f176d0_0 .var "reset", 0 0;
S_0x125f06fc0 .scope module, "uut" "PC" 2 14, 3 1 0, S_0x125f06e50;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "PCin";
    .port_info 3 /OUTPUT 32 "PCout";
v0x125f071e0_0 .net "PCin", 31 0, v0x125f174a0_0;  1 drivers
v0x125f17240_0 .var "PCout", 31 0;
v0x125f172f0_0 .net "clock", 0 0, v0x125f17600_0;  1 drivers
v0x125f173a0_0 .net "reset", 0 0, v0x125f176d0_0;  1 drivers
E_0x125f06d40 .event posedge, v0x125f172f0_0;
    .scope S_0x125f06fc0;
T_0 ;
    %wait E_0x125f06d40;
    %load/vec4 v0x125f173a0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x125f17240_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x125f071e0_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x125f17240_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x125f06e50;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x125f17600_0, 0, 1;
T_1.0 ;
    %delay 10000, 0;
    %load/vec4 v0x125f17600_0;
    %inv;
    %store/vec4 v0x125f17600_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x125f06e50;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x125f176d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x125f176d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %load/vec4 v0x125f17550_0;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %load/vec4 v0x125f17550_0;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %load/vec4 v0x125f17550_0;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x125f176d0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x125f176d0_0, 0, 1;
    %pushi/vec4 32, 0, 32;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %load/vec4 v0x125f17550_0;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %load/vec4 v0x125f17550_0;
    %store/vec4 v0x125f174a0_0, 0, 32;
    %delay 20000, 0;
    %vpi_call 2 62 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x125f06e50;
T_3 ;
    %vpi_call 2 67 "$dumpfile", "tb_PC/tb_pc.vcd" {0 0 0};
    %vpi_call 2 68 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x125f06e50 {0 0 0};
    %vpi_call 2 69 "$monitor", "At time %t, Reset = %b, PCin = %h, PCout = %h", $time, v0x125f176d0_0, v0x125f174a0_0, v0x125f17550_0 {0 0 0};
    %end;
    .thread T_3;
