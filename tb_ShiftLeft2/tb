    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 1073741824, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 3221225472, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 4294967295, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 2147483648, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 2147483647, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x11e707d80;
T_2 ;
    %vpi_call 2 48 "$dumpfile", "./tb_shift_left_2.vcd" {0 0 0};
    %vpi_call 2 49 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11e707d80 {0 0 0};
    %vpi_call 2 50 "$monitor", "At time %t, ShiftIn = %h (hex), ShiftOut = %h (hex)", $time, v0x11e71b870_0, v0x11e71b930_0 {0 0 0};
    %end;
    .thread T_2;
