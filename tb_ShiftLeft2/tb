S_0x11e707d80 .scope module, "ShiftLeft2_tb" "ShiftLeft2_tb" 2 3;
 .timescale -9 -12;
v0x11e71b870_0 .var "ShiftIn", 31 0;
v0x11e71b930_0 .net "ShiftOut", 31 0, v0x11e71b7b0_0;  1 drivers
S_0x11e707ef0 .scope module, "uut" "ShiftLeft2" 2 11, 3 1 0, S_0x11e707d80;
 .timescale -9 -12;
    .port_info 0 /INPUT 32 "ShiftIn";
    .port_info 1 /OUTPUT 32 "ShiftOut";
v0x11e705750_0 .net "ShiftIn", 31 0, v0x11e71b870_0;  1 drivers
v0x11e71b7b0_0 .var "ShiftOut", 31 0;
E_0x11e706180 .event anyedge, v0x11e705750_0;
    .scope S_0x11e707ef0;
T_0 ;
    %wait E_0x11e706180;
    %load/vec4 v0x11e705750_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x11e71b7b0_0, 0, 32;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x11e707d80;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x11e71b870_0, 0, 32;
    %delay 100000, 0;
